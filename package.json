{
  "name": "pokemon",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.28.0",
    "eslint-utils": "1.4.2",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.1.2",
    "redux": "4.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
