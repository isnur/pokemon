    ]
  },
  "devDependencies": {
    "axios-mock-adapter": "1.17.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js"
